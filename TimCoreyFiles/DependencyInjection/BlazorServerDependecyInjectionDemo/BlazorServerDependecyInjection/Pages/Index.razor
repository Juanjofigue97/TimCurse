@page "/"

@* @inject IDemo _demo
@inject IDemo _demo2 *@

@inject IEnumerable<IDemo> _demos
@inject ProcessDemo _processDemo

<PageTitle>Index</PageTitle>

<h1>Hello, world!</h1>

@foreach (var item in _demos)
{
    @if (item is ILocalDemo)
    {
        <h2>Local Demo @item.StartupTime.ToString("hh:mm:ss ffffff")</h2>
    }

    @if (item is IUtcDemo)
    {
        <h2>UTC Demo @item.StartupTime.ToString("hh:mm:ss ffffff")</h2>
    }

}


@* <h2>Demo Time: @_demo.StartupTime.ToString("hh:mm:ss ffffff")</h2>
<h2>Demo Time: @_demo2.StartupTime.ToString("hh:mm:ss ffffff")</h2> *@

<h2>Days in Month: @_processDemo.GetDaysInMonth()</h2>





Welcome to your new app.

