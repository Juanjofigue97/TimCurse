@page
@model RazorPageTimSolution.Pages.AddressesModel
@{
    ViewData["Title"] = "Address";
}
<h1>Address</h1>
<form method="post">
    <div class="form-group">
        <label class="col-form-label" asp-for="Address.HomeAddress"></label>
        <input type="text" asp-for="Address.HomeAddress" placeholder="Home " />
    </div>
    <div class="form-group">
        <label asp-for="Address.WorkAddress"></label>
        <input type="text" asp-for="Address.WorkAddress" placeholder="Work" />
    </div>
    <div class="form-group">
        <label asp-for="Address.SecondHouseAddress"></label>
        <input type="text" asp-for="Address.SecondHouseAddress" placeholder="Second House" />
    </div>
    <button type="submit" class="btn btn-success">Submit</button>
</form>